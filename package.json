{
  "name": "currency-test",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "apollo-boost": "^0.4.4",
    "apollo-client": "^2.6.4",
    "apollo-server": "^2.9.12",
    "apollo-server-express": "^2.9.12",
    "express": "^4.17.1",
    "graphql": "^14.5.8",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "3.2.0"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development concurrently \"nodemon -e js,graphql --watch server ./server/index.js\" \"react-scripts start\"",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^5.0.0",
    "cross-env": "^6.0.3",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react-hooks": "^2.3.0",
    "husky": "^3.1.0",
    "nodemon": "^2.0.1",
    "prettier": "^1.19.1",
    "prettier-eslint": "^9.0.1",
    "prettier-eslint-cli": "^5.0.0"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn test",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "eslint --fix",
      "git add",
      "eslint --max-warnings=0"
    ],
    "server/**/*.js": [
      "eslint --fix",
      "git add",
      "eslint --max-warnings=0"
    ]
  },
  "proxy": "http://localhost:4000"
}